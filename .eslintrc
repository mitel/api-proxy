{
  "extends": "eslint-config-airbnb",
  "env": {
    "mocha": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "padded-blocks": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-expressions": 0,
    "no-console": 0,
    "no-trailing-spaces": 0,
    "block-scoped-var": 0,
    "no-unused-vars": 0,
    "react/no-multi-comp": 0,
    // "quotes": 0
  }
}
